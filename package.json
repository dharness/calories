{
  "name": "calorie-optimizer",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "bun server/cli.ts",
    "api": "bun --hot server/server.ts",
    "dev": "bunx --bun concurrently -k \"bun --hot server/server.ts\" \"cd client && bunx --bun vite\"",
    "build": "cd client && bunx --bun vite build",
    "preview": "cd client && bunx --bun vite preview",
    "test:ingredient-data": "bun tests/ingredient_data_agent_test.ts",
    "test:ingredient-extractor": "bun tests/ingredient_extractor_agent_test.ts",
    "test:optimize": "bun tests/optimize_agent_test.ts",
    "test:profile": "bun --cpu-prof tests/ingredient_data_agent_test.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^1.1.0",
    "@langchain/google-genai": "^1.0.0",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "dotenv": "^16.4.5",
    "ink": "^6.6.0",
    "langchain": "^1.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.0",
    "prettier": "^3.0.0",
    "vite": "^7.1.3"
  }
}
